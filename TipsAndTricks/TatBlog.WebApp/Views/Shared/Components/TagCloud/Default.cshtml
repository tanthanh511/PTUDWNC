@model IList<TagItem> 

<div class="mb-4">
    <h3 class="text-success mb-2">
        danh sach the
    </h3>

    <ul class="list-group">
        @foreach (var item in Model)
        {
            <li class="list-group-item">
                <a asp-controller="Blog"
                   asp-action="Tag"
                   asp-route-slug="@item.UrlSlug">
                    @item.UrlSlug
                </a>
            </li>
        }
    </ul>
</div> 